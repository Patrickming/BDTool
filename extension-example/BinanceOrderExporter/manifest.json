{
  "manifest_version": 3,
  "name": "Binance Alpha Order Exporter",
  "version": "2.0.0",
  "description": "Export Binance Alpha trading order history",
  "permissions": ["storage", "downloads", "activeTab", "scripting"],
  "host_permissions": ["https://www.binance.com/*"],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png"
    },
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}
